- blog ||= feed_item.item

%div[feed_item]
  .left
  = image_tag('blurp_photos.png', :class => "icon_small", :title => "User Photo Icon", :alt => "User Photo Icon")
  = link_to h(blog.profile.f), blog.profile
  blogged
  = time_ago_in_words blog.created_at
  ago
  = link_to sanitize(textilize(blog.title)), profile_blog_path(blog.profile, blog)

  .right
    = x_feed_link(feed_item) if me

  .clear